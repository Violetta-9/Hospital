{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=Hospital;User ID=postgres;Password=;Pooling=true;",
    "BlobStorageConnection": "UseDevelopmentStorage=true;"
  },
  "JwtSettings": {
    "JwtSecretKey": ""
  },
  "BlobStorageSettings": {
    "DoctorPathTemplate": "doctor/{0}/images/{1}",
    "ReceptionistPathTemplate": "receptionist/{0}/images/{1}",
    "PatientPathTemplate": "patient/{0}/images/{1}",
    "OfficePathTemplate": "office/{0}/images/{1}",
    "ImagesContainer": "hospital-documents"
  }
}